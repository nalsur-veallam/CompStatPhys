set terminal epslatex color
set output "pic3.tex"
set format xy "$%g$"
set lmargin 10
set grid
set key bottom right

set title "Коэффициент самодиффузии от времени"
set xlabel "$\\tau$" 
set ylabel "$D$" offset -3, 0 rotate by 0
plot "log.vacf.T1.5.rho0.67.txt" using 1:3  title "T=1.5, \\rho=0.67" with linespoints, "log.vacf.T1.4.rho0.67.txt" using 1:3  title "T=1.4, \\rho=0.67" with linespoints, "log.vacf.T1.5.rho0.64.txt" using 1:3  title "T=1.5, \\rho=0.64" with linespoints, "log.vacf.T1.4.rho0.64.txt" using 1:3  title "T=1.4, \\rho=0.64" with linespoints, "log.vacf.T1.5.rho0.61.txt" using 1:3  title "T=1.5, \\rho=0.61" with linespoints, "log.vacf.T1.4.rho0.61.txt" using 1:3  title "T=1.4, \\rho=0.61" with linespoints

set out
